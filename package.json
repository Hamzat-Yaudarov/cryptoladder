{
  "name": "anya-dimension-matrix",
  "version": "1.0.0",
  "description": "Anya's Dimension Matrix - A mystical Telegram MiniApp",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run vite-dev\" \"npm run server-dev\"",
    "vite-dev": "vite --host",
    "server-dev": "nodemon server/index.js",
    "build": "vite build",
    "start": "npm run build && node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "telegraf": "^4.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0"
  }
}
